  object qryRel: TFDQuery
    Connection = frmPrincipal.ConexaoLocal
    FetchOptions.AssignedValues = [evRowsetSize]
    FetchOptions.RowsetSize = 1500000
    UpdateOptions.AssignedValues = [uvUpdateMode]
    UpdateOptions.UpdateMode = upWhereAll
    SQL.Strings = (
      '   SELECT d.descricao_produto,'
      '          SUM(d.quantidade) AS total'
      '     FROM venda_cabecalho c,'
      '          venda_detalhe d,'
      '          produtos p'
      '    WHERE c.id = d.id_venda_cabecalho'
      '      AND d.id_produto = p.id'
      '      AND c.status = '#39'F'#39
      '      AND c.data_hora_venda BETWEEN :dtIni AND :dtfim'
      '     GROUP BY d.descricao_produto, d.quantidade'
      '     ORDER BY SUM(d.quantidade) DESC')
    Left = 24
    Top = 149
    ParamData = <
      item
        Name = 'DTINI'
        DataType = ftTimeStamp
        ParamType = ptInput
      end
      item
        Name = 'DTFIM'
        DataType = ftTimeStamp
        ParamType = ptInput
      end>
    object qryRelDESCRICAO_PRODUTO: TStringField
      FieldName = 'DESCRICAO_PRODUTO'
      Origin = 'DESCRICAO_PRODUTO'
      Size = 120
    end
