    SQL.Strings = (
      'SELECT p.descricao,'
      '         u.sigla,'
      '         d.valor_unitario,'
      '         d.quantidade,'
      '         d.valor_total'
      
        '    FROM venda_detalhe d INNER JOIN produtos p ON d.id_produto =' +
        ' p.id'
      
        '                         INNER JOIN unidade_produtos u ON p.id_u' +
        'nidade = u.id'
      '      AND d.id_venda_cabecalho > 0')
