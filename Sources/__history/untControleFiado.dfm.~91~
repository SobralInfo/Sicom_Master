  object qryGrid: TFDQuery
    BeforeScroll = qryGridBeforeScroll
    AfterScroll = qryGridAfterScroll
    OnCalcFields = qryGridCalcFields
    Connection = frmPrincipal.ConexaoLocal
    UpdateOptions.AssignedValues = [uvUpdateMode]
    UpdateOptions.UpdateMode = upWhereAll
    SQL.Strings = (
      '  SELECT f.*,'
      '         c.nome,'
      '         c.logradouro,'
      '         c.numero,'
      '         c.complemento,'
      '         c.bairro,'
      '         ci.cid_nome'
      '    FROM fiados f INNER JOIN clientes c ON f.id_cliente = c.id'
      '                  LEFT JOIN cidade ci ON c.cidade = ci.id'
      '      AND f.id_cliente > 0'
      '   ORDER BY f.id DESC')
    Left = 240
    Top = 424
    object qryGridID: TIntegerField
      FieldName = 'ID'
      Origin = 'ID'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object qryGridID_VENDA: TIntegerField
      FieldName = 'ID_VENDA'
      Origin = 'ID_VENDA'
    end
    object qryGridVALOR_VENDA: TFloatField
      FieldName = 'VALOR_VENDA'
      Origin = 'VALOR_VENDA'
      DisplayFormat = '###,#0.00'
    end
    object qryGridDATA_VENDA: TSQLTimeStampField
      FieldName = 'DATA_VENDA'
      Origin = 'DATA_VENDA'
