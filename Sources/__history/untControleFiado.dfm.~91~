      '          c.nome,'
      '          c.fone_fixo,'
      '          c.celular,'
      '          c.logradouro,'
      '          c.numero,'
      '          c.complemento,'
      '          c.bairro,'
      '          c.bloqueado,'
      '          ci.cid_nome'
      '     FROM fiados f INNER JOIN clientes c ON f.id_cliente = c.id'
      
        '                   LEFT JOIN cidade ci ON cast(c.cidade AS varch' +
        'ar(7)) = ci.cid_codigo'
      ' ORDER BY c.nome')
