  object qryGrid: TFDQuery
    OnCalcFields = qryGridCalcFields
    Connection = frmPrincipal.ConexaoLocal
    UpdateOptions.AssignedValues = [uvUpdateMode]
    UpdateOptions.UpdateMode = upWhereAll
    SQL.Strings = (
      '            SELECT Distinct(c.id) as pedido,'
      '                   cl.nome,'
      '                   c.id_cliente,'
      '                   c.id_funcionario,'
      '                   Coalesce(c.valor_liquido,0) as valor_liquido,'
      '                   c.data_abertura,'
      '                   c.status,'
      '                   c.valor_total,'
      '                   c.valor_desconto,'
      '                   c.valor_liquido,'
      '                   c.contato,'
      '                   c.taxa_entrega,'
      '                   count(d.descricao) as valor_total,'
      '                   sum(d.valor_total) as  total_itens'
      '              FROM pedido_cabecalho c,'
      '                   pedido_detalhe d,'
      '                   clientes cl,'
      '                   funcionarios f'
      '             WHERE c.id = d.id_pedido'
      '               AND c.id_cliente = cl.id'
      '               AND c.id_funcionario = f.id'
      '               AND c.status = '#39'C'#39
      '          group by c.id,'
      '                   cl.nome,'
      '                   c.valor_liquido,'
      '                   c.data_abertura,'
      '                   c.id_cliente,'
      '                   c.id_funcionario,'
      '                   c.status,'
      '                   c.valor_total,'
      '                   c.valor_desconto,'
      '                   c.valor_liquido,'
      '                   c.contato,'
      '                   c.taxa_entrega'
      '          ORDER BY c.data_abertura DESC, c.id DESC')
    Left = 344
    Top = 288
    object qryGridPEDIDO: TIntegerField
      FieldName = 'PEDIDO'
      Origin = 'PEDIDO'
      Required = True
    end
    object qryGridNOME: TStringField
      FieldName = 'NOME'
      Origin = 'NOME'
