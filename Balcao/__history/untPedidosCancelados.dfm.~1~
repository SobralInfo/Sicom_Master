  object qryItens: TFDQuery
    OnCalcFields = qryGridCalcFields
    Connection = frmPrincipal.ConexaoLocal
    UpdateOptions.AssignedValues = [uvUpdateMode]
    UpdateOptions.UpdateMode = upWhereAll
    SQL.Strings = (
      '                SELECT d.*,'
      '                       p.descricao as produto,'
      '                       p.codigo_barras,'
      '                       u.sigla'
      '                  FROM pedido_detalhe d,'
      '                       produtos p,'
      '                       unidade_produtos u'
      '                 WHERE d.id_produto = p.id'
      '                   AND p.id_unidade = u.id'
      '                   AND d.id_pedido = 143')
    Left = 472
    Top = 288
